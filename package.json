{
  "name": "shogun-button-react",
  "description": "Shogun connector button",
  "version": "6.1.0",
  "files": [
    "dist",
    "src/styles/index.css"
  ],
  "keywords": [
    "shogun",
    "wallet",
    "ethereum",
    "react",
    "react-hook",
    "hooks",
    "authentication"
  ],
  "author": "Scobru",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && webpack --mode production && copyfiles -u 1 \"src/styles/index.css\" dist && node scripts/make-esm.mjs",
    "build:tsc": "tsc",
    "build:webpack": "webpack --mode production",
    "dev": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "ethers": "^6.13.5",
    "gun": "^0.2020.1241",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "rxjs": "^7.8.2",
    "shogun-core": "^6.1.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "typeRoots": [
    "./node_modules/@types",
    "./src/types"
  ]
